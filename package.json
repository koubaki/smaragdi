{
  "name": "smaragdi",
  "version": "0.3.2",
  "type": "module",
  "description": "A JavaScript framework with features like routing and SSR.",
  "scripts": {
    "build": "tsc && typedoc --options typedoc.json",
    "test": "node tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koubaki/smaragdi.git"
  },
  "author": "koubaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koubaki/smaragdi/issues"
  },
  "homepage": "https://github.com/koubaki/smaragdi#readme",
  "imports": {
    "#self": "./dist/index.js",
    "#self/router": {
      "node": "./dist/router/index.server.js",
      "browser": "./dist/router/index.client.js"
    },
    "#self/head": "./dist/head/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./router": {
      "node": "./dist/router/index.server.js",
      "browser": "./dist/router/index.client.js"
    },
    "./head": "./dist/head/index.js"
  },
  "dependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-streaming": "^0.4.2",
    "rollup": "^4.43.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typedoc": "^0.28.10",
    "typescript": "^5.8.3"
  }
